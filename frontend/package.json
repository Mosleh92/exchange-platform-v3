{
  "name": "exchange-platform-frontend",
  "version": "1.0.0",
  "description": "پلتفرم صرافی - Multi-Tenant SaaS Platform",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext js,jsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "performance": "lighthouse http://localhost:3000 --output=html --output-path=./performance-report.html"
  },
  "keywords": [
    "exchange",
    "currency",
    "saas",
    "multi-tenant",
    "persian",
    "rtl",
    "react"
  ],
  "author": "Exchange Platform Team",
  "license": "MIT",
  "dependencies": {
    "@mui/lab": "^7.0.0-beta.14",
    "@mui/x-charts": "^8.5.3",
    "axios": "^1.5.0",
    "chart.js": "^4.4.0",
    "clsx": "^2.0.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^10.16.4",
    "i18next": "^25.2.1",
    "lucide-react": "^0.292.0",
    "moment": "^2.29.4",
    "moment-jalaali": "^0.10.0",
    "notistack": "^3.0.2",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-countup": "^6.4.2",
    "react-datepicker": "^4.21.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^15.5.3",
    "react-intersection-observer": "^9.5.2",
    "react-number-format": "^5.3.1",
    "react-phone-number-input": "^3.3.6",
    "react-qr-code": "^2.0.12",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.8.1",
    "react-select": "^5.8.0",
    "react-table": "^7.8.0",
    "recharts": "^2.15.4",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^1.14.0",
    "@supabase/supabase-js": "^2.39.7"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.48.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
