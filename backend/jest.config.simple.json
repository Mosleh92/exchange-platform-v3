{
  "testEnvironment": "node",
  "setupFilesAfterEnv": [],
  "coverageDirectory": "coverage",
  "collectCoverageFrom": [
    "src/**/*.js",
    "!src/tests/**"
  ],
  "testMatch": [
    "**/tests/**/*.test.js"
  ],
  "moduleFileExtensions": ["js", "json"],
  "verbose": true,
  "testTimeout": 30000
}