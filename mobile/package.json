    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace ExchangePlatform.xcworkspace -scheme ExchangePlatform -configuration Release -destination generic/platform=iOS -archivePath ExchangePlatform.xcarchive archive",
